{
	"name": "BlogADFv2sec",
	"properties": {
		"activities": [
			{
				"name": "CopySQLtoADLSgen2",
				"type": "ForEach",
				"dependsOn": [
					{
						"activity": "SaveProcessMetaData",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@pipeline().parameters.cw_items",
						"type": "Expression"
					},
					"activities": [
						{
							"name": "CopyTable2ADLSgen2",
							"type": "Copy",
							"dependsOn": [
								{
									"activity": "GetTableStructure",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [
								{
									"name": "Source",
									"value": "@{item().source.schema}.@{item().source.table}"
								},
								{
									"name": "Destination",
									"value": "dbtest1//@{item().destination.fileName}"
								}
							],
							"typeProperties": {
								"source": {
									"type": "AzureSqlSource",
									"partitionOption": "None"
								},
								"sink": {
									"type": "DelimitedTextSink",
									"storeSettings": {
										"type": "AzureBlobFSWriteSettings"
									},
									"formatSettings": {
										"type": "DelimitedTextWriteSettings",
										"quoteAllText": true,
										"fileExtension": ".txt"
									}
								},
								"enableStaging": false
							},
							"inputs": [
								{
									"referenceName": "BlogADFvsecSQL",
									"type": "DatasetReference",
									"parameters": {
										"cw_schema": "@item().source.schema",
										"cw_table": "@item().source.table"
									}
								}
							],
							"outputs": [
								{
									"referenceName": "BlogADFvsecADLSgen2",
									"type": "DatasetReference",
									"parameters": {
										"cw_fileName": "@item().destination.fileName"
									}
								}
							]
						},
						{
							"name": "GetTableStructure",
							"type": "GetMetadata",
							"dependsOn": [],
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"dataset": {
									"referenceName": "BlogADFvsecSQL",
									"type": "DatasetReference",
									"parameters": {
										"cw_schema": "@item().source.schema",
										"cw_table": "@item().source.table"
									}
								},
								"fieldList": [
									"structure",
									"columnCount"
								]
							}
						},
						{
							"name": "Write metadata to folder app - Vnet REST API",
							"type": "Copy",
							"dependsOn": [
								{
									"activity": "CopyTable2ADLSgen2",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"source": {
									"type": "RestSource",
									"httpRequestTimeout": "00:01:40",
									"requestInterval": "00.00:00:00.010",
									"requestMethod": "POST",
									"requestBody": {
										"value": "@{concat('{\"tableStructureArray\":', string(activity('GetTableStructure').output.structure), ',\"processMetaDataStor\":', string(variables('processMetaDataStor')), ',\"tableNameArray\":\"', string(concat(item().source.schema, item().source.table)), '\",\"ADLSgen2stor\":\"', string(pipeline().parameters.ADLSGen2Stor), '\",\"filepath\":\"', concat(pipeline().parameters.ADLSGen2FileSystem,'/', item().destination.fileName, '/', utcnow('yyyy'), '/', utcnow('MM'), '/', utcnow('dd')), '\"}')}",
										"type": "Expression"
									}
								},
								"sink": {
									"type": "JsonSink",
									"storeSettings": {
										"type": "AzureBlobFSWriteSettings"
									},
									"formatSettings": {
										"type": "JsonWriteSettings"
									}
								},
								"enableStaging": false
							},
							"inputs": [
								{
									"referenceName": "BlogADFvsecREST",
									"type": "DatasetReference"
								}
							],
							"outputs": [
								{
									"referenceName": "BlogADFvsecADLSgen2JSON",
									"type": "DatasetReference",
									"parameters": {
										"cw_fileName": "@item().destination.fileName"
									}
								}
							]
						}
					]
				}
			},
			{
				"name": "SaveProcessMetaData",
				"type": "SetVariable",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"variableName": "processMetaDataStor",
					"value": [
						{
							"firstRow": {
								"retentionPeriod": 62,
								"sourceSystem": "bremerov",
								"dataowner": "rebremer",
								"privacyLevel": "10",
								"generationData": "2019-09-09 16:56:00Z"
							}
						}
					]
				}
			}
		],
		"parameters": {
			"cw_items": {
				"type": "Array",
				"defaultValue": [
					{
						"source": {
							"schema": "dbo",
							"table": "BuildVersion"
						},
						"destination": {
							"fileName": "dboBuildVersion.txt"
						}
					},
					{
						"source": {
							"schema": "dbo",
							"table": "ErrorLog"
						},
						"destination": {
							"fileName": "dboErrorLog.txt"
						}
					},
					{
						"source": {
							"schema": "SalesLT",
							"table": "Address"
						},
						"destination": {
							"fileName": "SalesLTAddress.txt"
						}
					},
					{
						"source": {
							"schema": "SalesLT",
							"table": "Customer"
						},
						"destination": {
							"fileName": "SalesLTCustomer.txt"
						}
					},
					{
						"source": {
							"schema": "SalesLT",
							"table": "CustomerAddress"
						},
						"destination": {
							"fileName": "SalesLTCustomerAddress.txt"
						}
					},
					{
						"source": {
							"schema": "SalesLT",
							"table": "Product"
						},
						"destination": {
							"fileName": "SalesLTProduct.txt"
						}
					},
					{
						"source": {
							"schema": "SalesLT",
							"table": "ProductCategory"
						},
						"destination": {
							"fileName": "SalesLTProductCategory.txt"
						}
					},
					{
						"source": {
							"schema": "SalesLT",
							"table": "ProductModelProductDescription"
						},
						"destination": {
							"fileName": "SalesLTProductModelProductDescription.txt"
						}
					},
					{
						"source": {
							"schema": "SalesLT",
							"table": "SalesOrderDetail"
						},
						"destination": {
							"fileName": "SalesLTSalesOrderDetail.txt"
						}
					},
					{
						"source": {
							"schema": "SalesLT",
							"table": "SalesOrderHeader"
						},
						"destination": {
							"fileName": "SalesLTSalesOrderHeader.txt"
						}
					}
				]
			},
			"ADLSGen2FileSystem": {
				"type": "string",
				"defaultValue": "sqldbdata"
			},
			"ADLSGen2Stor": {
				"type": "string",
				"defaultValue": "az500adfsecstr"
			}
		},
		"variables": {
			"tableStructureArray": {
				"type": "Array"
			},
			"tableNameArray": {
				"type": "Array"
			},
			"processMetaDataStor": {
				"type": "Array"
			}
		},
		"annotations": []
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}